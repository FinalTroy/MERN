{
    "compilerOptions": {
      "target": "ES6",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,
      "noEmit": true,
      "jsx": "react",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ]
    },
    "include": [
      "models",
      "routes"
    ]
  }
  